// Sleep in Bed Scripts
script AsaSleepBed {
    lock
    questmenu(QUEST_MENU_CHECK_COMPLETE, ADVENTURE_BEGINS)
    if (var(VAR_RESULT) == TRUE) {
        delay(20)
        checkplayergender
        if (var(VAR_RESULT) == MALE) {
            goto(PlayerBed)
        } else {
            applymovement(OBJ_EVENT_ID_PLAYER, WalkRight)
            waitmovement(0)
            goto(RivalBed)
        }
    } else {
        checkplayergender
        if (var(VAR_RESULT) == MALE) {
            goto(ShouldNotSleepYet)
        } else {
            delay(20)
            applymovement(OBJ_EVENT_ID_PLAYER, WalkRight)
            waitmovement(0)
            goto(RivalBed)
        }
    }
}

script AriaSleepBed {
    lock
    questmenu(QUEST_MENU_CHECK_COMPLETE, ADVENTURE_BEGINS)
    if (var(VAR_RESULT) == TRUE) {
        delay(20)
        checkplayergender
        if (var(VAR_RESULT) == MALE) {
            applymovement(OBJ_EVENT_ID_PLAYER, WalkLeft)
            waitmovement(0)
            goto(RivalBed)
        } else {
            goto(PlayerBed)
        }
    } else {
        checkplayergender
        if (var(VAR_RESULT) == MALE) {
            delay(20)
            applymovement(OBJ_EVENT_ID_PLAYER, WalkLeft)
            waitmovement(0)
            goto(RivalBed)
        } else {
            goto(ShouldNotSleepYet)
        }
    }
}

script PlayerBed {
	if (flag(FLAG_SYS_REAL_TIME) == FALSE) {
		applymovement(OBJ_EVENT_ID_PLAYER, FaceDown)
		waitmovement(0)
		//applymovement(OBJ_EVENT_ID_PLAYER, Think)
		//waitmovement(0)
		msgbox(format("What time should I sleep until?"), MSGBOX_DEFAULT)
		waitmessage
		multichoice(25, 2, MULTI_TIME_OF_DAY, 0)
		switch (var(VAR_RESULT)) {
			case 0:
				call(MorningSleep)
			case 1:
				call(AfternoonSleep)
			case 2:
				call(EveningSleep)
			case 3:
				call(NightSleep)
			case 4:
			default:
				closemessage
				delay(20)
				checkplayergender
				if (var(VAR_RESULT) == MALE) {
					applymovement(OBJ_EVENT_ID_PLAYER, WalkRight)
					waitmovement(0)  
				} else {
					applymovement(OBJ_EVENT_ID_PLAYER, WalkLeft)
					waitmovement(0)
				}
				//applymovement(OBJ_EVENT_ID_PLAYER, Think)
				//waitmovement(0)
				msgbox(format("No time to sleep!\n I'm on an adventure!"), MSGBOX_DEFAULT)
				closemessage
				end
		}
		fadescreen(1)
		closemessage
		playse(SE_EXP_MAX)
		waitse
		delay(10)
		playse(SE_EXP_MAX)
		waitse
		delay(10)
		playse(SE_EXP_MAX)
		waitse
		delay(10)
		special(HealPlayerParty)
		fadescreen(0)
		delay(20)
		switch (var(VAR_RESULT)) {
			case 0:
				msgbox(format("{PLAYER} WOKE UP AT 9AM"), MSGBOX_DEFAULT)
			case 1:
				msgbox(format("{PLAYER} WOKE UP AT 1PM"), MSGBOX_DEFAULT)
			case 2:
				msgbox(format("{PLAYER} WOKE UP AT 6PM"), MSGBOX_DEFAULT)
			case 3:
				msgbox(format("{PLAYER} WOKE UP AT 10PM"), MSGBOX_DEFAULT)
		}
		closemessage
		checkplayergender
		if (var(VAR_RESULT) == MALE) {
			applymovement(OBJ_EVENT_ID_PLAYER, WalkRight)
			waitmovement(0)  
		} else {
			applymovement(OBJ_EVENT_ID_PLAYER, WalkLeft)
			waitmovement(0)
		}
		release
	} else {
		call(PlayerReturnToLastPosition)
		msgbox(format("No time to sleep!\n I'm on an adventure!"), MSGBOX_SIGN)
		end
	}
}

