script PlayersHouse_1F_EventScript_Espurr_ChooseGender {
	delay(30)
	msgbox(format("MOM: That's our ESPURR, whilst one of the most adorable POKéMON I've ever,\l ESPURR is such a rambunctious scamp.\p I've been calling ESPURR “NORTI TORTI”,\n but we need to settle on a name.\l What do you think?"), MSGBOX_DEFAULT)
	multichoice(4, 8, MULTI_POPPY_PEDRO, TRUE)
	if (var(VAR_RESULT) == POPPY) {
		setflag(FLAG_ESPURR_GENDER)
		msgbox(format("MOM: I knew it, that's the most wonderful name for her.\p ESPURR is so very shy, but I just know some adventure would do her good…"), MSGBOX_DEFAULT)
	} elif (var(VAR_RESULT) == PEDRO) {
		msgbox(format("MOM: I knew it, that's the most wonderful name for him.\p ESPURR is so very shy, but I just know some adventure would do him good…"), MSGBOX_DEFAULT)
	}
	setflag(FLAG_CHOSEN_ESPURR_GENDER)
	closemessage
	playmoncry(SPECIES_ESPURR, 0)
	waitmoncry

	if (flag(FLAG_SET_HOMELY_STATE) == FALSE) {
		goto(PlayersHouse_1F_EventScript_Espurr_GiveBeforeHomely)
	} else {
		goto(PlayersHouse_1F_EventScript_Espurr_GiveAfterHomely)
	}
}

script PlayersHouse_1F_EventScript_Espurr_GiveBeforeHomely {
	bufferleadmonspeciesname(STR_VAR_1)
	if (flag(FLAG_ESPURR_GENDER) == TRUE) {
		msgbox(format("MOM: Wow, it sounds like ESPURR enjoys hanging out with {STR_VAR_1}.\p Maybe she can go with you on your adventure? She wouldn't battle, but,\l she could get out and see the world.\p Do you like the sound of that ESPURR?"), MSGBOX_DEFAULT)
	} else {
		msgbox(format("MOM: Wow, it sounds like ESPURR enjoys hanging out with {STR_VAR_1}.\p Maybe he can go with you on your adventure? He wouldn't battle, but,\l he could get out and see the world.\p Do you like the sound of that ESPURR?"), MSGBOX_DEFAULT)
	}
	waitmoncry
	closemessage
	playmoncry(SPECIES_ESPURR, 0)
	waitmoncry
	if (flag(FLAG_ESPURR_GENDER) == TRUE) {
		msgbox(format("MOM: It sounds like she's really up for this experience with you!\l Is that okay, will you take ESPURR?"), MSGBOX_YESNO)
	} else {
		msgbox(format("MOM: It sounds like she's really up for this experience with you!\l Is that okay, will you take ESPURR?"), MSGBOX_YESNO)
	}
	if (var(VAR_RESULT) == NO) {
		msgbox(format("Don't worry, ESPURR will be here if you change your mind.\l Take good care, {PLAYER}!"), MSGBOX_DEFAULT)
	} else {
		if (flag(FLAG_ESPURR_GENDER) == TRUE) {
			msgbox(format("Here, let me return her POKéBALL to you.\n Think of a name for her on your travels.\l Take good care of her, {PLAYER}!"), MSGBOX_DEFAULT)
		} else {
			msgbox(format("Here, let me return him POKéBALL to you.\n Think of a name for him on your travels.\l Take good care of him, {PLAYER}!"), MSGBOX_DEFAULT)
		}
		giveitem(ITEM_ESPURR_POKEBALL, 1)
		setcaughtmon(SPECIES_ESPURR)
		setflag(FLAG_HIDE_BRENDANS_HOUSE_ESPURR)
		setflag(FLAG_HIDE_MAYS_HOUSE_ESPURR)
	}
	closemessage
	end
}

