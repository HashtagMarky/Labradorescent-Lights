script PlayersHouse_Homely_EventScript_Common_BeginTwo {
	waitmovement(0)
	msgbox(format("DAD: Today's the day!\n I'm glad you're well rested,\l I won't be going easy on you!"), MSGBOX_DEFAULT)
	closemessage
	delay(20)
	msgbox(format("MOM: I'm looking forward to seeing you express yourself, {PLAYER}."), MSGBOX_DEFAULT)
	closemessage
	delay(20)
	applymovement(LOCALID_MOM, Homely_Talk_FacingDown)
